{
  "name": "sls-bookly",
  "version": "1.1.0",
  "description": "a serverless proof-of-concept: a simple app for managing lists of books and authors. this repo is for both the backend api and the front-end react client",
  "private": "true",
  "main": "authors.js",
  "dependencies": {
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.9",
    "moxios": "^0.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "reset-css": "^5.0.1",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.1",
    "css-loader": "^3.5.3",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "build": "sls webpack",
    "mongo": "mongod --config /usr/local/etc/mongod.conf",
    "invoke-list-authors": "./scripts/invoke/list-authors.sh",
    "invoke-show-author": "./scripts/invoke/show-author.sh",
    "test": "EXECUTION_CONTEXT=express jest --watch",
    "build:client": "webpack --config webpack.client.config.js --mode production",
    "start:client": "webpack-dev-server --config webpack.client.config.js --mode development",
    "start:server": "EXECUTION_CONTEXT=express nodemon --exec babel-node ./src/server/index.js --watch src/server --watch src/serverless",
    "db:seed": "babel-node ./scripts/db/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-daveatsho:showtime-digitalmedia/sls-bookly.git"
  },
  "author": "",
  "license": "ISC"
}
